<%- include('./shared/header') %>
        <h1>About Us</h1>
    </header>
    <section class="content-container">
        <div class="text-container">
        <h2>Our Story</h2>
        <p>Welcome to our News Portal! We are dedicated to...</p>

        <h2>Our Mission</h2>
        <p>Our mission is to give you latest info...</p>

        <h2>Meet the Team</h2>
        <p>Get to know the faces behind our success:</p>
        <div class="container">

        <% teamMembers.forEach(function(member) { %>
            <div class="row py-2">
                <div class="col-sm">
            <%- member.firstName %>
            <%- member.lastName %><br/>
            <%- member.position %><br/>
            <a href="<%- member.github %>">Github Profile</a>
           </div>
                <div class="col-sm">
                </div>
            </div>
          <% }); %>
        </div>
    </div>
    <div class="image-container">
    <img class="image" src="/images/news-image.jpg" alt="news on mobile"/>
    </div>
    </section>
    <%- include('./shared/footer') %>

